[getpwd.c]
/* Crawl the /etc/passwd file */
#include &lt;pwd.h&gt; 
int main()
{
struct passwd *p; 
while(
p=getpwent())
printf(&quot;%s:%s:%d:%d:%s:%s:%s\n&quot;, p-&gt;pw_name,p-&gt;pw_passwd,
p-&gt;pw_uid, p-&gt;pw_gid, p-&gt;pw_gecos, p-&gt;pw_dir, p-&gt;pw_shell);
}


