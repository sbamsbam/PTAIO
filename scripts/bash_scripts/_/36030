[Getevaddr.c]

#include &lt;stdio.h&gt;
#include &lt;stdlib.h&gt;
#include &lt;string.h&gt;

int main(int argc, char *argv[]) {
        char *ptr;

        if(argc &lt; 2) {
                printf(&quot;Usage: %s &lt;environment variable&gt; \n&quot;, argv[0]);
                exit(0);
        }
        ptr = getenv(argv[1]); 
        printf(&quot;%s will be at %p\n&quot;, argv[1], ptr);
}


