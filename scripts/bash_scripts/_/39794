;Sleep
    push byte +0x70  							; 00000171 6A70
    push dword 0x65656c53  						; 00000173 68536C6565
    push esp  								; 00000178 54
    push ebx  								; 00000179 53
    call edi  								; 0000017A FFD7
    lea esp,[esp+0x8]  							; 0000017C 8D642408
