&quot;\x72\x04&quot;           	      /*  moveq #4,%d1          */
&quot;\x4e\x40&quot;           	      /*  trap #0               */
&quot;\xb1\x80&quot;           	      /*  eorl %d0,%d0          */  
&quot;\x2f\x40\x00\x04&quot;     	      /*  movel %d0,%sp@(4)     */ 
&quot;\x2f\x40\x00\x08&quot;     	      /*  movel %d0,%sp@(8)     */
&quot;\x70\x66&quot;           	      /*  moveq #102,%d0        */
&quot;\x72\x05&quot;           	      /*  moveq #5,%d1          */ 
&quot;\x4e\x40&quot;           	      /*  trap #0               */
&quot;\x22\x00&quot;           	      /*  movel %d0,%d1         */
&quot;\x70\x3f&quot;           	      /*  moveq #63,%d0         */
