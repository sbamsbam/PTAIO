              -- this=Lcom/johny/rootwine/RootWine; &lt;831930596104&gt;
           ...
        ## trace thread &lt;1&gt; main        (running suspended)
           -- com.johny.rootwine.RootWine.isRooted()Z:0
              -- this=Lcom/johny/rootwine/RootWine; &lt;831930596104&gt;
           ...
        ## trace thread &lt;1&gt; main        (running suspended)
           -- com.johny.rootwine.RootWine.detectRootManagementApps()Z:0
              -- this=Lcom/johny/rootwine/RootWine; &lt;831930596104&gt;
