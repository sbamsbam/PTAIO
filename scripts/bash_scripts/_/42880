Move(){
echo &quot;Basic movement in $1:&quot;
echo
echo &quot;j - down (or rotate)&quot;
echo &quot;k - up (or rotate)&quot;
echo &quot;h - left&quot;
echo &quot;l - right&quot;
echo &quot;q or Q to quit&quot;
echo
echo &quot;[RETURN]\c&quot;
read ret
}

