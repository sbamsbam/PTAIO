 104:	ff d3                	call   rbx
 106:	41 c6 07 02          	mov    BYTE PTR [r15],0x2
 10a:	66 41 c7 47 02 db 83 	mov    WORD PTR [r15+0x2],0x83db
 111:	41 c7 47 04 c1 a1 c1 	mov    DWORD PTR [r15+0x4],0x63c1a1c1
