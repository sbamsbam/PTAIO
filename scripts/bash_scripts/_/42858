*** 510,516 ****
		malloc_strcpy(&amp;my_path, &quot;/&quot;);
	if (*realname == null(char))
		strmcpy(realname, &quot;*Unknown*&quot;, REALNAME_LEN);
!       if (*username == null(char))
	{
		if (ptr = getenv(&quot;USER&quot;))
			strmcpy(username, ptr, NAME_LEN);
--- 510,518 ----
